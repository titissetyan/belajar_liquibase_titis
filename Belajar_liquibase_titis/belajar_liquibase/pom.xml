<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">
		 
		 <changeSet id="1" author="titis">
        <createTable tableName="tabel-liquibase">
            <column name="nis" type="int(5)">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="nama" type="varchar(20)">
                <constraints nullable="false"/>
            </column>
            <column name="kelas" type="varchar(10)">
				<constraints nullable="true"/>
			</column>
			<column name="jurusan" type="varchar(15)">
				<constraints nullable="true"/>
			</column>
			<column name="jenis_kelamin" type="varchar(10)">
				<constraints nullable="true"/>
			</column>
			<column name="alamat" type="text">
				<constraints nullable="true"/>
			</column>
			
        </createTable>
    </changeSet>


</databaseChangeLog>
